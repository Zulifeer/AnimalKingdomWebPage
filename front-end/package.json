    "dev:webpack": "webpack --mode development -w",
    "build:webpack": "webpack --mode production",
    "watch:sass": "node-sass src/SASS/main.scss ../src/main/resources/static/CSS/style.css -w",
    "devserver": "live-server --browser=firefox",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass src/SASS/main.scss ../src/main/resources/static/CSS/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" ../src/main/resources/static/CSS/style.comp.css -o ../src/main/resources/static/CSS/style.prefix.css",
    "compress:css": "node-sass ../src/main/resources/static/CSS/style.prefix.css ../src/main/resources/static/CSS/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
